# =====================================================
# WisdomOS Multi-Agent System (MAS) Configuration
# FD-v5 Environment Variables
# =====================================================

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_ANON_KEY=your-anon-key

# Database
DATABASE_URL=postgresql://postgres:password@localhost:54322/postgres

# AI/ML APIs
OPENAI_API_KEY=sk-your-openai-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key

# Analytics & Monitoring
POSTHOG_KEY=phc_your-posthog-key
LOGSNAG_TOKEN=your-logsnag-token

# Security & Encryption
ENCRYPTION_MASTER_KEY=base64-encoded-32-byte-key
JWT_SECRET=your-jwt-secret

# Timezone & Scheduling
TZ=America/Toronto
ROLLUP_SCHEDULE_HOUR=2
ROLLUP_SCHEDULE_TIMEZONE=America/Toronto

# Agent Configuration
AGENT_POLL_INTERVAL_MS=5000
AGENT_MAX_CONCURRENT_JOBS=10
AGENT_MAX_RETRIES=3
AGENT_RETRY_BACKOFF=exponential

# Feature Flags
ENABLE_COMMITMENT_AUTO_SPAWN=true
ENABLE_NARRATIVE_AUTO_GENERATION=true
ENABLE_INTEGRITY_MONITORING=true
ENABLE_FINANCE_AUTO_IMPORT=false
ENABLE_JUSTICE_SYNC=false

# Thresholds
COMMITMENT_CONFIDENCE_THRESHOLD=0.75
CLASSIFICATION_CONFIDENCE_THRESHOLD=0.7
AREA_SIMILARITY_THRESHOLD=0.8

# Security
INTEGRITY_TIME_LOCK_DAYS=90
ENTRY_EDIT_GRACE_PERIOD_DAYS=7

# Integration URLs
HUBSPOT_API_KEY=your-hubspot-key
QUICKBOOKS_CLIENT_ID=your-quickbooks-client-id
QUICKBOOKS_CLIENT_SECRET=your-quickbooks-secret

# Deployment
NODE_ENV=development
LOG_LEVEL=info
SENTRY_DSN=your-sentry-dsn

# IP & Branding
COMPANY_NAME=King Legend Inc.
PRODUCT_LINE=PVT Hostel Products
PLATFORM_NAME=WisdomOS
VERSION=5.0.0
